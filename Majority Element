MOORE ALGORITHM IS OPTIMAL ONE
#include<iostream>
#include<vector>
using namespace std;

int main(){
    int n;
    cin>>n;
    int arr[100];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int m=n/2,temp;
    int count=0;
    for(int i=0;i<n;i++){
	for(int j=0;j<n;j++){
		if(arr[i]=arr[j])
		count++;
	}
	if(count>m)
		temp=arr[i];
	}
	cout<<temp<<endl;
		return 0;
	}
