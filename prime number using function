#include<stdio.h>
int isPrime(int X){
int i,c=0;
for(i=1;i<=X;i++){
    if(X%i==0){
        c++;
    }
}

if(c==2)
return 1;
else 
return 0;
}
int main(){
int a,b;
scanf("%d %d ",&a,&b);
int ans =isPrime(a);
if(ans==1){
    printf("%d is prime\n",a);
}
ans =isPrime(b);
if(ans==1){
    printf("%d is  prime\n",b);
}
return 0;
}
